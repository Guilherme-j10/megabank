n    return(\r\n        <>\r\n            <GlobalStyle />\r\n            <Style.grid style={{marginTop: top, marginBottom: bottom, marginRight: right, marginLeft: left, transform: `rotate(${rotate})`}} className=\"grid\">\r\n                {rows}\r\n            </Style.grid>\r\n        </>\r\n    );\r\n}","C:\\Users\\Developer\\Documents\\megabank\\src\\pages\\Deashboard\\index.js",["101","102"],"C:\\Users\\Developer\\Documents\\megabank\\src\\pages\\Deashboard\\style.js",[],"C:\\Users\\Developer\\Documents\\megabank\\src\\validate.js",[],"C:\\Users\\Developer\\Documents\\megabank\\src\\config\\api.js",[],"C:\\Users\\Developer\\Documents\\megabank\\src\\pages\\components\\screenLoad\\index.js",["103"],"import React from 'react';\r\nimport GlobalStyle from '../../style/GlobalStyle';\r\nimport * as Style from './style';\r\n\r\nexport default function ScreenLoad(){\r\n    return(\r\n        <>\r\n            <GlobalStyle />\r\n            <Style.allScreen>\r\n                <i class=\"fas fa-circle-notch fa-spin\"></i>\r\n                <h1>Carregando...</h1>\r\n            </Style.allScreen>\r\n        </>\r\n    );\r\n}","C:\\Users\\Developer\\Documents\\megabank\\src\\pages\\components\\screenLoad\\style.js",[],"C:\\Users\\Developer\\Documents\\megabank\\src\\pages\\Account\\index.js",["104"],"C:\\Users\\Developer\\Documents\\megabank\\src\\pages\\components\\templates\\sidebar\\index.js",["105","106"],"import React from 'react';\r\nimport { Link } from 'react-router-dom';\r\n\r\nimport LogoImg from '../../../assets/logo.png';\r\n\r\nexport default function SideBar(){\r\n    return(\r\n        <div className=\"left\">\r\n            <span className=\"dotsBubble\"></span>\r\n            <span className=\"dotsBubble Dois\"></span>\r\n            <span className=\"dotsBubble Tres\"></span>\r\n            <section className=\"global\">\r\n                <header className=\"headerLogo\">\r\n                    <img src={LogoImg} />\r\n                </header>\r\n                <ul className=\"OptionMenuUser\">\r\n                    <li className=\"ativo\"><Link to={{pathname: '/home', from: window.location.href}}><i class=\"fas fa-tachometer-alt\"></i> Deashboard</Link></li>\r\n                    <li className=\"special\">\r\n                        <span className=\"sideLeftOfTheForce\">\r\n                            <i class=\"fas fa-chart-pie\"></i>\r\n                        </span>\r\n                        <ul className=\"OptionAnalytics\">\r\n                            <li><strong>Analise</strong></li>\r\n                            <li><Link>Conteudo</Link></li>\r\n                            <li><Link>Engajamento</Link></li>\r\n                            <li><Link>Ganhos</Link></li>\r\n                        </ul>\r\n                    </li>\r\n                </ul>\r\n            </section>\r\n            <section className=\"sectionUser\">\r\n                <a onClick={(e)=>{localStorage.removeItem('tokenMegaBank'); window.location.href = '/';}} ><i class=\"fas fa-sign-out-alt\"></i>Sair</a>\r\n            </section>\r\n        </div>\r\n    );\r\n}",["107","108"],"C:\\Users\\Developer\\Documents\\megabank\\src\\pages\\components\\screens\\Account\\index.js",["109","110","111","112","113","114"],"import React, { useEffect, useRef, useState } from 'react';\r\n\r\nimport * as Style from './style';\r\n\r\nimport api from '../../../../config/api';\r\n\r\nimport { ToastContainer, toast } from 'react-toastify';\r\nimport 'react-toastify/dist/ReactToastify.css';\r\n\r\nexport default function AccountScreen(){\r\n\r\n    const [nameFileTemp, setNameFileTemp] = useState();\r\n    \r\n    const [Nome, setNome] = useState('');\r\n    const [Email, setEmail] = useState('');\r\n    const [Senha, setSenha] = useState('');\r\n    const [Cargo, setCargo] = useState('');\r\n    const [Pathname, setPathname] = useState('');\r\n\r\n    useEffect(() => {\r\n        loadDadosUser();\r\n    }, []);\r\n\r\n    function loadDadosUser(){\r\n        (async() => {\r\n            const userData = await api.get('/getFuldata', {\r\n                headers: {\r\n                    token: localStorage.getItem('tokenMegaBank')\r\n                }\r\n            });\r\n            \r\n            setNome(userData.data.nome)\r\n            setEmail(userData.data.email)\r\n            setSenha(userData.data.senha)\r\n            setCargo(userData.data.cargo)\r\n            setPathname(userData.data.pathname)\r\n        })();\r\n    }\r\n\r\n    function previewImg(e){\r\n        e.preventDefault();\r\n\r\n        let preview = document.getElementById('img');\r\n        preview.src = URL.createObjectURL(e.target.files[0]);\r\n    }\r\n\r\n    function sendData(e){\r\n        e.preventDefault();\r\n\r\n        const file = e.target.file.files[0];\r\n\r\n        if(file == null){\r\n            SendDados({\r\n                nome: e.target.nome.value,\r\n                email: e.target.email.value,\r\n                senha: e.target.senha.value,\r\n                cargo: e.target.cargo.value,\r\n            }, 'data');\r\n        }else{\r\n            const data = new FormData();\r\n            data.append('profileimg', e.target.file.files[0]);\r\n\r\n            SendDados(data, 'file');\r\n            SendDados({\r\n                nome: e.target.nome.value,\r\n                email: e.target.email.value,\r\n                senha: e.target.senha.value,\r\n                cargo: e.target.cargo.value,\r\n            }, 'data');\r\n        }\r\n\r\n        async function SendDados(Dados, Type){\r\n            const enviaDados = await api.post('/updateDados', Dados, {\r\n                headers: {\r\n                    token: localStorage.getItem('tokenMegaBank')\r\n                }\r\n            });\r\n\r\n            console.log(enviaDados.data);\r\n\r\n            if(Type == 'data'){\r\n                if(enviaDados.data == true){\r\n                    (()=>{\r\n                        toast.info(\"Dados atualizados\", {position: \"bottom-right\"})\r\n                    })()\r\n                }else{\r\n                    (()=>{\r\n                        toast.error(\"Desculpe, tente novamente mais tarde\", {position: \"bottom-center\"})\r\n                    })()\r\n                }\r\n            }\r\n        };\r\n    }\r\n\r\n    return(\r\n        <Style.Main>\r\n            <form onSubmit={(e) => {sendData(e)}} className=\"firstSpace\">\r\n                <div className=\"sideProfileImg\">\r\n                    <img className=\"ProfileImg\" src={Pathname} id=\"img\" />\r\n                    <div className=\"dadosForm\" encType=\"multipart/form-data\">\r\n                        <label for=\"file\">Selecionar Imagem</label>\r\n                        <input type=\"file\" onChange={(e) => {previewImg(e)}} id=\"file\" name=\"profile\" />\r\n                    </div>\r\n                </div>\r\n                <div className=\"Global\">\r\n                    <h1>Editar minha conta</h1>\r\n                    <div className=\"inputs\">\r\n                        <div className=\"sideFlat\">\r\n                            <i class=\"fas fa-user\"></i>\r\n                            <input type=\"text\" name=\"nome\" value={Nome} onChange={(e)=>{setNome(e.target.value)}} placeholder=\"Digite seu nome completo\"/>\r\n                        </div>\r\n                        <div className=\"sideFlat\">\r\n                            <i class=\"fas fa-user\"></i>\r\n                            <input type=\"email\" name=\"email\" value={Email} onChange={(e)=>{setEmail(e.target.value)}} placeholder=\"Digite seu e-mail\"/>\r\n                        </div>\r\n                        <div className=\"sideFlat\">\r\n                            <i class=\"fas fa-user\"></i>\r\n                            <input type=\"password\" name=\"senha\" value={Senha} onChange={(e)=>{setSenha(e.target.value)}} placeholder=\"Digite sua senha\"/>\r\n                        </div>\r\n                        <div className=\"sideFlat\">\r\n                            <i class=\"fas fa-user\"></i>\r\n                            <input type=\"text\" name=\"cargo\" value={Cargo} onChange={(e)=>{setCargo(e.target.value)}} placeholder=\"Qual é o seu cargo ?\"/>\r\n                        </div>\r\n                    </div>\r\n                    <input type=\"submit\" value=\"ATUALIZAR DADOS\" />\r\n                    <ToastContainer />\r\n                </div>\r\n            </form>\r\n        </Style.Main>\r\n    );\r\n}","C:\\Users\\Developer\\Documents\\megabank\\src\\pages\\components\\screens\\home\\index.js",[],"C:\\Users\\Developer\\Documents\\megabank\\src\\pages\\components\\screens\\Account\\style.js",[],"C:\\Users\\Developer\\Documents\\megabank\\src\\pages\\components\\templates\\sidebarDois\\index.js",[],"C:\\Users\\Developer\\Documents\\megabank\\src\\pages\\components\\screens\\home\\style.js",[],{"ruleId":"115","severity":1,"message":"116","line":1,"column":17,"nodeType":"117","messageId":"118","endLine":1,"endColumn":26},{"ruleId":"119","severity":1,"message":"120","line":29,"column":15,"nodeType":"121","messageId":"122","endLine":29,"endColumn":17},{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","replacedBy":"126"},{"ruleId":"115","severity":1,"message":"127","line":4,"column":16,"nodeType":"117","messageId":"118","endLine":4,"endColumn":24},{"ruleId":"128","severity":1,"message":"129","line":53,"column":29,"nodeType":"130","endLine":53,"endColumn":50},{"ruleId":"131","severity":1,"message":"132","line":62,"column":99,"nodeType":"130","endLine":62,"endColumn":102},{"ruleId":"131","severity":1,"message":"132","line":63,"column":100,"nodeType":"130","endLine":63,"endColumn":103},{"ruleId":"123","replacedBy":"133"},{"ruleId":"125","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":18,"column":13,"nodeType":"130","endLine":18,"endColumn":159},{"ruleId":"137","severity":1,"message":"138","line":26